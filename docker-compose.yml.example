version: '3.8'
services:
      expressapp:
            build: .
            ports:
                  - "OUTSIDE-PORT-TO-USE:3000"
            environment:
                  - TEMP_IMAGES_PATH=/Local-Images
                  - UPLOADED_PATH=/server/Local-Images
                  - NO_HTTPS=false
            volumes:
                  - DIRECTORY-TO-SAVE-IMAGES-TO:/server/Local-Images